 import java.util.*;
public class Basics{


  
  public static void Patterntype2d(int n){
    
      for(int i=1;i<=n;i++){
         
        for(int j=1;j<=n;j++){
        int k=n-i+1;
        System.out.print(k);
          }
          
        System.out.println();
      }
  }
  public static void main(String args[]){
      Scanner sc=new Scanner(System.in);
      System.out.println("Enter range");
      int n=sc.nextInt();
      Patterntype2d(n);
  }
}

